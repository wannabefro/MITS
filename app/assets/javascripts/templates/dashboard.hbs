<h3>My Teams</h3>
<hr>
{{#each teams}}
  {{#link-to 'team.index' this}}{{name}}{{/link-to}}
{{else}}
  <p>You currently have no teams</p>
{{/each}}
<h3>Recent Mits</h3>
<hr>
{{#each recentMits}}
  <h4>{{group}}</h4>
  {{#each content}}
    {{#link-to 'mit' this}}{{title}}{{/link-to}}<br>
  {{/each}}
{{else}}
   <p>No recent Mits</p>
{{/each}}
{{-graph data=groupedStats rawData=stats}}
